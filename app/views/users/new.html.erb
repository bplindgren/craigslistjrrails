<h2>Create your new account!</h2>

<%= form_tag(controller: "users", action: "create", method: "post") %>
  <%= label_tag("Username:") %>
  <%= text_field_tag 'user[username]' %>
  <%= label_tag("Email:") %>
  <%= text_field_tag 'user[email]' %>
  <%= label_tag("Password:") %>
  <%= text_field_tag 'user[password]' %>
  <%= submit_tag("Submit") %>

